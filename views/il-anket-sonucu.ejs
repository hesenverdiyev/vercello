            <div class="container" id="firstcontainer"> 
                 
<% let totalVotes = 0; %>
<% let candidatesWithVotes = []; %>
<% for (let i = 0; i < candidates.length; i++) { %>
  <% let citylist = []; %>
  <% for (let j = 0; j < candidates[i].followers.length; j++) { %>
    <% citylist.push(candidates[i].followers[j].city); %>
  <% } %>
  <% const filteredCities = citylist.filter(city => city.includes(cityName));%>
  <% const candidateWithVotes = { fullname: candidates[i].fullname, votes: filteredCities.length }; %>
  <% candidatesWithVotes.push(candidateWithVotes); %>
  <% totalVotes += filteredCities.length; %>
<% } %>
<% candidatesWithVotes.sort((a, b) => b.votes - a.votes); %>
<ol class="my-list">
  <% for (let i = 0; i < candidatesWithVotes.length; i++) { %>
    <% let votePercentage = (candidatesWithVotes[i].votes / totalVotes) * 100; %>
    <li><%= candidatesWithVotes[i].fullname %> (<%= candidatesWithVotes[i].votes %> oy)
      <div class="chart" style="width: <%= (votePercentage + 7).toFixed(1) %>%;">
        <%= votePercentage.toFixed(1) %>%</div>
    </li>
  <% } %>
</ol>
<p><b>Toplam ankete katılanların sayısı: <u><%= totalVotes %></u></b></p>
